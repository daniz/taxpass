      %td
        != App.help "input", type: "number", id: "interest_profit_abroad_25", model: @
      %td
        != App.help "input", type: "number", id: "interest_profit_abroad_35", model: @

    %tr
      %td רווחים פטורים
      %td
        != App.help "input", type: "number", id: "interest_exempt_profit_0", model: @
      %td
        != App.help "input", type: "number", id: "interest_exempt_profit_10", model: @
      %td
        != App.help "input", type: "number", id: "interest_exempt_profit_15", model: @
      %td
        != App.help "input", type: "number", id: "interest_exempt_profit_20", model: @
      %td
