- prefix = if @spouse then "spouse_" else "" 

%input{ "class": "#{ prefix }workplace", type: "text", placeholder: "מקום העבודה" }
%input{ "class": "#{ prefix }106_file_#{ @index }", type: "file" }
%button.f106-upload-button.button.button-empty{ type: "button" } העלה קובץ 106
  %i.fa.fa-check
%button.f106-manual-button.button.button-empty{ type: "button" } הזן ידנית
  %i.fa.fa-check